{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/proxy/:path*",
      "destination": "/api/proxy?proxyPath=:path*"
    },
    {
      "source": "/api/sync/:path*",
      "destination": "/api/sync?proxyPath=:path*"
    }
  ]
}
